    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "nodemon": "^3.1.9",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
